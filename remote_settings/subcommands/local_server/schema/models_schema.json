      "properties": {
        "hash": {
          "type": "string",
          "title": "Hash"
        },
        "size": {
          "type": "number",
          "title": "Size (bytes)"
        },
        "filename": {
          "type": "string",
          "title": "Filename"
        },
        "location": {
          "type": "string",
          "title": "URL"
        },
        "mimetype": {
          "type": "string",
          "title": "MIME type"
        },
        "original": {
          "type": "object",
          "title": "Pre-gzipped file",
          "properties": {
            "hash": {
              "type": "string",
              "title": "Hash"
            },
            "size": {
              "type": "number",
              "title": "Size (bytes)"
            },
            "filename": {
              "type": "string",
              "title": "Filename"
            },
            "mimetype": {
              "type": "string",
              "title": "MIME type"
            }
          },
          "additionalProperties": false
        }
      },
      "description": "Information about the attached file.",
      "additionalProperties": false
