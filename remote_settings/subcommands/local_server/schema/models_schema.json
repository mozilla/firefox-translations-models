    "decompressedSize": {
      "type": "number",
      "title": "Size (decompressed, bytes)",
      "description": "The size of the decompressed file"
    },
    "sourceLanguage": {
      "type": "string",
      "title": "Source Language",
      "description": "The BCP 47 language tag of the source text"
    },
    "attachment": {
